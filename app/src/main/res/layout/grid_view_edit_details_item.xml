<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/relallinfo"
        android:paddingTop="1dp"
        android:paddingLeft="1dp"
        android:background="@drawable/borderrows"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="horizontal"
            android:weightSum="1">
            <ImageView
                android:id="@+id/btnedit"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="0.1"
                android:padding="1dp"
                android:layout_marginRight="5dp"
                android:src="@drawable/editentrynx"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="75dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:gravity="center"
                android:text="AdmNo"
                android:layout_marginLeft="1dp"
                android:textColor="@android:color/black" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />
            <TextView
                android:id="@+id/txt_AdmDt"
                android:layout_width="172dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="Adm.Date"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />

            <TextView
                android:id="@+id/textView_rollno"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:gravity="center"
                android:layout_marginLeft="1dp"
                android:text="Name"
                android:lines="3"
                android:textColor="@android:color/black" />
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />

            <TextView
                android:id="@+id/textView_fn"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
               android:layout_weight="0.1"
                android:text="FATHERNAME"
                android:gravity="center"
                android:lines="3"
                android:textStyle="bold" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/textView_mn"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:text="MOTHERNAME"
                android:gravity="center"
                android:lines="3"
                android:textColor="#0026C2"/>
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/textView_dob"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:text="DoB"
                android:gravity="center"
                android:lines="3"
                android:textColor="#0026C2"/>

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />
            <TextView
                android:id="@+id/txt_Mob"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="Mobile#"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_IFSC"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="IFSC"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />
            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_ACNum"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="BANK A/C #"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />
            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_Gen"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="GENDER"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_Ward"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="WARD-VILLAGE"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_Cat"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="CATGORY"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_Minority"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="MINORITY"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_Handi"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="HANDICAPPED"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_AdNum"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="#"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />
            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_AdName"
                android:layout_width="174dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.1"
                android:textColor="#0026C2"
                android:text="#"
                android:gravity="center"
                android:lines="2"
                android:textStyle="bold" />

            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary" />


            <TextView
                android:id="@+id/txt_reason"
                android:layout_width="190dp"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:textColor="#0026C2"
                android:text="#"
                android:gravity="left"
                android:lines="3"
                android:textStyle="normal" />
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignRight="@+id/relallinfo"
        android:layout_gravity="top">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnList"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:layout_margin="2dp"
            android:visibility="gone"
            android:orientation="horizontal">
            <!--<ImageView-->
                <!--android:id="@+id/btnedit"-->
                <!--android:layout_width="45dp"-->
                <!--android:layout_height="45dp"-->
                <!--android:padding="1dp"-->
                <!--android:layout_marginRight="5dp"-->
                <!--android:src="@drawable/editentrynx"/>-->
            <ImageView
            android:id="@+id/btnupload"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_marginRight="10dp"
            android:visibility="visible"
            android:src="@drawable/uploadnx"/>
            <!--<ImageView-->
            <!--android:id="@+id/btndelete"-->
            <!--android:layout_width="45dp"-->
            <!--android:layout_height="55dp"-->
            <!--android:src="@drawable/deleteentrynx"/>-->
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/relallinfo"
        android:layout_gravity="top">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnListLeft"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:layout_margin="2dp"
            android:visibility="gone"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/btneditleft"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:padding="1dp"
                android:layout_marginRight="5dp"
                android:src="@drawable/editentrynx"/>
            <ImageView
                android:id="@+id/btnuploadleft"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginRight="10dp"
                android:visibility="visible"
                android:src="@drawable/uploadnx"/>
            <!--<ImageView-->
            <!--android:id="@+id/btndelete"-->
            <!--android:layout_width="45dp"-->
            <!--android:layout_height="55dp"-->
            <!--android:src="@drawable/deleteentrynx"/>-->
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>